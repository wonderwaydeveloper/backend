APP_NAME="Clevlance"
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_URL=https://api.clevlance.com
APP_PORT=80
APP_SSL_PORT=443

FRONTEND_URL=https://clevlance.com

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=error

DB_CONNECTION=mysql
DB_HOST=mysql
DB_PORT=3306
DB_DATABASE=clevlance
DB_USERNAME=clevlance
DB_PASSWORD=
DB_ROOT_PASSWORD=

BROADCAST_DRIVER=reverb
CACHE_DRIVER=redis
FILESYSTEM_DISK=local
QUEUE_CONNECTION=redis
SESSION_DRIVER=redis
