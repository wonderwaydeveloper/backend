# Search
SCOUT_DRIVER=meilisearch
MEILISEARCH_HOST=http://meilisearch:7700
MEILISEARCH_KEY=

# Admin
ADMIN_EMAIL=admin@clevlance.com
ADMIN_NAME=Administrator
ADMIN_PASSWORD=

# Security
CAPTCHA_ENABLED=true
